{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://chat-backend-rho-sable.vercel.app/api/$1"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "https://chat-backend-rho-sable.vercel.app/socket.io/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "continue": true
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "https://chat-backend-rho-sable.vercel.app"
  }
}